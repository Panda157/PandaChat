<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPaneStart" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.pandastudios.chatengine.fxGraphics.clientGui.startPackage.StartScene">
    <children>
        <Label alignment="CENTER" layoutX="76.0" layoutY="14.0" prefHeight="97.0" prefWidth="147.0" text="Willkommen beim Chat!" textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label layoutX="47.0" layoutY="111.0" prefHeight="17.0" prefWidth="219.0" text="Bitte wähle wie du fortfahren möchtest" />
        <Button fx:id="buttonLogin" layoutX="101.0" layoutY="150.0" mnemonicParsing="false" onAction="#loginUser" prefHeight="25.0" prefWidth="97.0" text="Login">
            <font>
                <Font size="17.0" />
            </font>
        </Button>
        <Button fx:id="buttonRegister" layoutX="101.0" layoutY="213.0" mnemonicParsing="false" onAction="#registerUser" prefHeight="25.0" prefWidth="97.0" text="Register">
            <font>
                <Font size="17.0" />
            </font>
        </Button>
        <Button fx:id="buttonClose" layoutX="278.0" layoutY="1.0" mnemonicParsing="false" style="-fx-background-color: transparent;" />
    </children>
</AnchorPane>
